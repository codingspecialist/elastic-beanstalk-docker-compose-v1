{
    "containerDefinitions": [
        {
            "command": [
                "./start.sh"
            ],
            "essential": true,
            "image": "openjdk:11",
            "mountPoints": [
                {
                    "containerPath": "/tmp",
                    "sourceVolume": "_BuildLibs"
                }
            ],
            "name": "backend",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 80
                }
            ],
            "privileged": true
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "./build/libs"
            },
            "name": "_BuildLibs"
        }
    ]
}
