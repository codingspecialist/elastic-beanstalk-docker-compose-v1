{
    "containerDefinitions": [
        {
            "command": [
                "java",
                "-jar",
                "blog-action-aws-0.0.1-SNAPSHOT.jar"
            ],
            "essential": true,
            "image": "openjdk:11",
            "memory": 2048,
            "mountPoints": [
                {
                    "containerPath": "/tmp",
                    "sourceVolume": "_BuildLibs"
                }
            ],
            "name": "backend",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 80
                }
            ],
            "privileged": true
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "./build/libs"
            },
            "name": "_BuildLibs"
        }
    ]
}
